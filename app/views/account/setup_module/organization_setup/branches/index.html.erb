<%= render :partial => "/account/setup_module/organization_setup/partials/container_header" %>
        <div class="container-fluid">
        	<div class="btn-group pull-right">
			  <a href="<%= new_account_setup_module_organization_setup_branch_path %>" class="btn btn-success">
			    <i class="icon-plus icon-white"></i> Create New Branch
			  </a>
			</div>
        </div>
        <br/>
		<%= grid(@branches_grid, :class => "row-fluid") do |g|

  			g.column :column_name => "Name", :attribute_name => "name" do |branch|
    			branch.name
  			end

  			g.column :column_name => "Address", :attribute_name => "address" do |branch|
    			branch.address
  			end

  			g.column :column_name => "Contact Number", :attribute_name => "contact_number" do |branch|
    			branch.contact_number
  			end

  			g.column do |branch|
    			link_to "Show", account_setup_module_organization_setup_branch_path(branch), :class => "btn btn-mini"
  			end

  			g.column do |branch|
				link_to "Edit", edit_account_setup_module_organization_setup_branch_path(branch), :class => "btn btn-mini"
  			end

  			g.column do |branch|
				link_to "Delete", account_setup_module_organization_setup_branch_path(branch), :confirm => 'Are you sure?', :method => :delete, :class => "btn btn-mini btn-danger"
  			end

		end -%>

		<%= include_wice_grid_assets %>
<%= render :partial => "/account/setup_module/organization_setup/partials/container_footer" %>