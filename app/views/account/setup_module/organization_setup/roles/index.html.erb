<%= render :partial => "/account/setup_module/organization_setup/partials/container_header" %>
        <div class="container-fluid">
        	<div class="btn-group pull-right">
			  <a href="<%= new_account_setup_module_organization_setup_role_path %>" class="btn btn-light-blue">
			    <i class="icon-plus icon-white"></i> Create New Role
			  </a>
			</div>
        </div>
        <br/>
		<%= grid(@roles_grid, :class => "row-fluid") do |g|

  			g.column :column_name => "Name", :attribute_name => "name" do |role|
    			role.name
  			end

  			g.column do |role|
    			link_to "Show", account_setup_module_organization_setup_concept_path(role), :class => "btn btn-mini"
  			end

  			g.column do |role|
				link_to "Edit", edit_account_setup_module_organization_setup_concept_path(role), :class => "btn btn-mini"
  			end

  			g.column do |role|
				link_to "Delete", account_setup_module_organization_setup_concept_path(role), :confirm => 'Are you sure?', :method => :delete, :class => "btn btn-mini btn-danger"
  			end

		end -%>

		<%= include_wice_grid_assets %>
<%= render :partial => "/account/setup_module/organization_setup/partials/container_footer" %>